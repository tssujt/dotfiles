{
    "cSpell.userWords": [
        "appname",
        "gunicorn"
    ],
    "cSpell.language": "en,en-US,en-GB",
    "cSpell.enabledLanguageIds": [
        "asciidoc",
        "c",
        "cpp",
        "csharp",
        "css",
        "django-html",
        "django-txt",
        "dockerfile",
        "git-commit",
        "go",
        "handlebars",
        "html",
        "jade",
        "java",
        "javascript",
        "javascriptreact",
        "jinja",
        "json",
        "jsonc",
        "latex",
        "less",
        "markdown",
        "php",
        "pip-requirements",
        "plaintext",
        "pug",
        "restructuredtext",
        "rust",
        "scala",
        "scss",
        "text",
        "toml",
        "typescript",
        "typescriptreact",
        "yaml",
        "yml"
    ],
    "editor.cursorBlinking": "smooth",
    "editor.fontSize": 19,
    "editor.fontFamily": "'Operator Mono'",
    "editor.largeFileOptimizations": false,
    "editor.lineHeight": 20,
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "all",
    "editor.rulers": [88, 120],
    "editor.smoothScrolling": true,
    "editor.suggestSelection": "first",
    // "editor.wordWrap": "bounded",
    "explorer.confirmDelete": false,
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        "Cargo.lock": "toml",
        "Cargo.toml": "toml"
    },
    "files.autoSave": "off",
    "files.autoSaveDelay": 10000,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/__pycache__": true,
        "**/.idea": true
    },
    "eslint.trace.server": "messages",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "leetcode.defaultLanguage": "python3",
    "leetcode.endpoint": "leetcode",
    "prettier.disableLanguages": [],
    "python.analysis.disabled": [
        "too-many-function-arguments",
        "parameter-missing"
    ],
    "python.analysis.errors": ["undefined-variable"],
    "python.analysis.information": ["unresolved-import"],
    "python.analysis.logLevel": "Trace",
    "python.analysis.openFilesOnly": false,
    "python.analysis.warnings": ["unknown-parameter-name"],
    "python.autoComplete.addBrackets": true,
    // "python.formatting.provider": "black",
    "python.formatting.autopep8Args": [
        "--max-line-length",
        "88"
    ],
    "python.jediEnabled": false,
    "python.linting.enabled": true,
    "python.linting.flake8Args": [
        "--format=pylint",
        "--count",
        "--verbose"
    ],
    "python.linting.flake8Enabled": true,
    "python.linting.ignorePatterns": [
        ".vscode/*.py",
        "**/site-packages/**/*.py"
    ],
    "python.linting.lintOnSave": true,
    "python.linting.pylintArgs": [
        "--load-plugins",
        "pylint_django"
    ],
    "python.pipenvPath": "/usr/local/bin/pipenv",
    "python.pythonPath": "${env:HOME}/.pyenv/shims/python",
    "python.sortImports.path": "${env:HOME}/.pyenv/shims/isort",
    "python.sortImports.args": [],
    "python.terminal.activateEnvironment": true,
    "python.testing.cwd": "tests",
    "python.testing.pytestEnabled": true,
    "python.venvPath": "${workspaceFolder}",
    "python.venvFolders": [
        "${workspaceFolder}/venv"
    ],
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.fontSize": 15,
    "terminal.integrated.fontFamily": "Source Code Pro for Powerline",
    "terminal.integrated.macOptionIsMeta": true,
    "vim.enableNeovim": true,
    "vim.neovimPath": "nvim",
    "window.openFilesInNewWindow": "on",
    "window.openFoldersInNewWindow": "on",
    "window.zoomLevel": 1,
    "workbench.activityBar.visible": true,
    "workbench.editor.enablePreview": false,
    "workbench.startupEditor": "none",
    // Enable per-language
    "[javascript]": {
        "editor.formatOnSave": false,
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[python]": {
        "editor.formatOnSave": false
    },
    "editor.cursorWidth": 5,
    "editor.cursorSmoothCaretAnimation": true,
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "terminal.integrated.shell.osx": "/bin/zsh",
    "workbench.colorTheme": "Solarized Light",
    "workbench.settings.editor": "json",
    "[vue]": {
        "editor.defaultFormatter": "octref.vetur"
    },
    "git.ignoreLegacyWarning": true,
    "python.languageServer": "Microsoft"
}
